@model List<Post>

<div class="card mb-3">
    <div class="card-header">
        <h5 class="card-title mb-0">Latest Posts</h5>
    </div>
    <div class="card-body">
            @foreach (var post in Model)
            {
                <div class="mb-3">
                    <h6 class="mb-6"><a href="/post/in-detail/@post.Url">@post.Title</a></h6>
                </div>
            }
    </div>
</div>